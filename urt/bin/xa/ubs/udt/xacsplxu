#!/usr/bin/php
<?php
$env = include	__DIR__.'/../../../../epc/xu/ubs/env.php';

use Unisrc\xu\lib\txt\Fb;
use Unisrc\xa\ubs\zzz\udt\xacsplxu;

Fb::setGenericOutputHandler();

$argv = $_SERVER['argv'];
$doit = !!(isset($argv[1]) && $argv[1] == 'doit');

xacsplxu::_($doit);

if(!$doit){
	echo <<< EOT
---
MODE = dry-run
To actually perform these actions:
$ xacsplxu doit

EOT;
}
